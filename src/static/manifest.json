{
  "manifest_version": 3,
  "name": "Your chrome extension name",
  "description": "Awesome chrome extension",
  "version": "1.0.0",
  "icons": {
    "16": "icon256.png",
    "32": "icon256.png",
    "48": "icon256.png",
    "128": "icon256.png"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "webRequest",
    "storage",
    "identity",
    "identity.email",
    "webNavigation",
    "cookies"
  ],
  "host_permissions": ["*://*.wanted.co.kr/*"],
  "action": {
    "default_icon": {
      "16": "icon256.png",
      "32": "icon256.png",
      "48": "icon256.png",
      "128": "icon256.png"
    },
    "default_title": "My Extension",
    "default_popup": "popup.html"
  }
}
